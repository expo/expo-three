{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "assets/*": ["./assets/*"],
      "expo-three": ["../src/index"],
      "expo-three/*": ["../src/*"],
      "three": ["./node_modules/three"],
      "three/*": ["./node_modules/three/*"]
    },
    "typeRoots": ["node_modules/@types", "../node_modules/@types"],
    "types": ["three"]
  }
}
